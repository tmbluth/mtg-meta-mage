            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"message\": \"Help me optimize my sideboard\",\n  \"conversation_id\": null,\n  \"context\": {\n    \"format\": \"Modern\",\n    \"archetype\": null,\n    \"days\": null,\n    \"deck_text\": \"4 Lightning Bolt\\n4 Lava Spike\\n4 Rift Bolt\\n4 Skewer the Critics\\n4 Monastery Swiftspear\\n4 Eidolon of the Great Revel\\n4 Goblin Guide\\n4 Spark Elemental\\n20 Mountain\"\n  }\n}"
            },
